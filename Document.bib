  shorttitle = {Fast {{Convolutional Neural Network Training Using Selective Data Sampling}}},
  author = {family=Grinsven, given=Mark J. J. P., prefix=van, useprefix=true and family=Ginneken, given=Bram, prefix=van, useprefix=true and Hoyng, Carel B. and Theelen, Thomas and Sánchez, Clara I.},
  date = {2016-05},
  journaltitle = {IEEE Transactions on Medical Imaging},
  volume = {35},
  number = {5},
