  booktitle = {2005 {{Seventh IEEE Workshops}} on {{Applications}} of {{Computer Vision}} ({{WACV}}/{{MOTION}}'05) - {{Volume}} 1},
  author = {Pallawala, P. M. D. S. and Hsu, W. and Lee, M. L. and Goh, S. S.},
  date = {2005-01},
