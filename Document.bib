  file = {/home/clement/Zotero/storage/ISSVFGK3/10.1109@RBME.2010.2084567.pdf;/home/clement/Zotero/storage/QDRTSMAE/Abràmoff et al. - 2010 - Retinal Imaging and Image Analysis.pdf}
}

