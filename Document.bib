  file = {/home/clement/Zotero/storage/EM5YY3J5/Wu et al. - 2019 - A Comprehensive Survey on Graph Neural Networks.pdf;/home/clement/Zotero/storage/96MKTB8K/1901.html}
}

@article{wuJCSExplainableCOVID192021,
