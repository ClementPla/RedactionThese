  author = {Sahlsten, Jaakko and Jaskari, Joel and Kivinen, Jyri and Turunen, Lauri and Jaanio, Esa and Hietala, Kustaa and Kaski, Kimmo},
  date = {2019-07-24},
  journaltitle = {Scientific Reports},
  shortjournal = {Sci Rep},
