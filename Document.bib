@article{dreoRobustRigidRegistration2006,
  title = {Robust Rigid Registration of Retinal Angiograms through Optimization},
  author = {Dréo, Johann and Nunes, Jean-Claude and Siarry, Patrick},
  date = {2006-12-01},
  journaltitle = {Computerized Medical Imaging and Graphics},
  shortjournal = {Computerized Medical Imaging and Graphics},
  volume = {30},
