  author = {Schmidt-Erfurth, Ursula and Waldstein, Sebastian M. and Klimscha, Sophie and Sadeghipour, Amir and Hu, Xiaofeng and Gerendas, Bianca S. and Osborne, Aaron and Bogunović, Hrvoje},
  date = {2018-07-02},
  journaltitle = {Investigative Ophthalmology \& Visual Science},
  shortjournal = {Investigative Ophthalmology \& Visual Science},
